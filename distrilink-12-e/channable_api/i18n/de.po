#. module: account
#: model:mail.template,body_html:account.email_template_edi_invoice
msgid "<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        % if object.type == 'out_invoice':\n"
"                            Dear Customer,\n"
"                            <br/><br/>\n"
"                            % if object.sale_order_id and object.sale_order_id.tracking_urls:\n"
"                                Your Distrilink order ${object.marketplace_id or 'n/a'} via ${object.sale_order_id.channable_channel_id.description} is shipped and you can track the delivery via: \n"
"                                <a target=\"_blank\" href=\"${object.sale_order_id.tracking_urls}\" style=\"color: #875A7B;\">${object.sale_order_id.tracking_urls}</a>\n"
"                            % else:\n"
"                                Your Distrilink order ${object.marketplace_id or 'n/a'} is shipped\n"
"                            % endif\n"
"                            <br/><br/>\n"
"                            <b>If you have questions about your delivery, please contact the courier service via your track &amp; trace url.</b>\n"
"                            <br/><br/>\n"
"                            Attached you can find the invoice of your order.\n"
"                            <br/><br/>\n"
"                            For service-related questions, please get in touch via service@distrilink.be.\n"
"                            <br/><br/>\n"
"                            Enjoy your purchase!\n"
"                            <br/><br/>\n"
"                            Best regards,\n"
"                            <br/>\n"
"                            Distrilink customer service\n"
"                        % endif\n"
"                        % if object.type == 'out_refund':\n"
"                            Hi,\n"
"                            <br/><br/>\n"
"                            Please find attached your credit note.\n"
"                            <br/><br/>\n"
"                            At your service!\n"
"                            <br/><br/>\n"
"                            Best regards,\n"
"                            <br/>\n"
"                            Distrilink customer service\n"
"                        % endif\n"
"                        % if object.type in ['in_invoice', 'in_refund']:\n"
"                            Dear ${object.partner_id.name}\n"
"                            % if object.partner_id.parent_id:\n"
"                                (${object.partner_id.parent_id.name})\n"
"                            % endif\n"
"                            <br/><br/>\n"
"                            Here is your \n"
"                            % if object.number:\n"
"                                invoice <strong>${object.number}</strong>\n"
"                            % else:\n"
"                                invoice\n"
"                            %endif\n"
"                            % if object.origin:\n"
"                                (with reference: ${object.origin})\n"
"                            % endif\n"
"                            amounting in <strong>${format_amount(object.amount_total, object.currency_id)}</strong>\n"
"                            from ${object.company_id.name}.\n"
"                            % if object.state=='paid':\n"
"                                This invoice is already paid.\n"
"                            % else:\n"
"                                Please remit payment at your earliest convenience.\n"
"                            % endif\n"
"                            <br/><br/>\n"
"                            Do not hesitate to contact us if you have any question.\n"
"                        % endif\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr "<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        % if object.type == 'out_invoice':\n"
"                            Sehr geehrter Kunde,\n"
"                            <br/><br/>\n"
"                            % if object.sale_order_id and object.sale_order_id.tracking_urls:\n"
"                                Ihre Distrilink Bestellung ${object.marketplace_id or 'n/a'} via
${object.sale_order_id.channable_channel_id.description} wird ausgeliefert und Sie können die Lieferung verfolgen via: \n"
"                                <a target=\"_blank\" href=\"${object.sale_order_id.tracking_urls}\" style=\"color: #875A7B;\">${object.sale_order_id.tracking_urls}</a>\n"
"                            % else:\n"
"                                Ihre Distrilink Bestellung ${object.marketplace_id or 'n/a'} wird ausgeliefert\n"
"                            % endif\n"
"                            <br/><br/>\n"
"                            <b>Wenn Sie Fragen zu Ihrer Lieferung haben, wenden Sie sich bitte über Ihre Track & Trace-URL an den Kurierdienst.</b>\n"
"                            <br/><br/>\n"
"                            Attached Sie finden die Rechnung Ihrer Bestellung.\n"
"                            <br/><br/>\n"
"                            Bei service bezogenen Fragen bitte melde dich via service@distrilink.be.\n"
"                            <br/><br/>\n"
"                            Viel Spaß beim Einkauf!\n"
"                            <br/><br/>\n"
"                            mit freundlichen Grüßen,\n"
"                            <br/>\n"
"                            Distrilink Kundenservice\n"
"                        % endif\n"
"                        % if object.type == 'out_refund':\n"
"                            Hallo,\n"
"                            <br/><br/>\n"
"                            Ihre Gutschrift finden Sie im Anhang.\n"
"                            <br/><br/>\n"
"                            \n"
"                            <br/><br/>\n"
"                            mit freundlichen Grüßen,\n"
"                            <br/>\n"
"                            Distrilink Kundenservice\n"
"                        % endif\n"
"                        % if object.type in ['in_invoice', 'in_refund']:\n"
"                            "\n"
"                           Sehr geehrte(r) ${object.partner_id.name}\n"
"                           % if object.partner_id.parent_id:\n"
"                               (${object.partner_id.parent_id.name})\n"
"                           % endif\n"
"                           \n"
"                           Anliegend erhalten Sie\n"
"                           % if object.number:\n"
"                               invoice ${object.number}\n"
"                           % else:\n"
"                               invoice\n"
"                           % endif\n"
"                           % if object.origin:\n"
"                               (with reference: ${object.origin})\n"
"                           % endif\n"
"                           in Höhe von <strong>${format_amount(object.amount_total, object.currency_id)}</strong>\n"
"                           von ${object.company_id.name}.\n"
"                           % if object.state=='paid':\n"
"                               Die Rechnung ist bereits bezahlt\n"
"                           % else:\n"
"                               Bitte überweisen Sie den Rechnungsbetrag so bald wie möglich.\n"
"                           % endif\n"
"                           <br/><br/>\n"
"                           Bei Fragen sprechen Sie uns gerne an.\n"
"                        % endif\n"
"                    </p>\n"
"                </div>\n"
"            "

