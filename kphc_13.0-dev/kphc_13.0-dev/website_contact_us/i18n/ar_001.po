"        <font style=\"font-size:24px;\">\n"
"            <a href=\"/shop\" target=\"_blank\" style=\"background-color:transparent;color:var(--brand-secondary);text-decoration-thickness:initial;\">\n"
"                SHOP NOW\n"
"            </a>\n"
