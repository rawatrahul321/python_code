<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="seq_inward_material" model="ir.sequence">
            <field name="name">Inward Material</field>
            <field name="code">inward.material</field>
            <field name="prefix">IW</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_production_process" model="ir.sequence">
            <field name="name">Production Process</field>
            <field name="code">production.process</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_production_new_lots" model="ir.sequence">
            <field name="name">Serial Numbers</field>
            <field name="code">stock.barcode.serial</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
